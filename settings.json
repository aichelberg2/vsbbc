{
  "code-runner.runInTerminal": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.saveFileBeforeRun": true,
  "debug.onTaskErrors": "debugAnyway",
  "explorer.confirmDelete": false,
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.detectIndentation": false,
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "files.associations": {
    "*.cs": "csharp"
  },
  "editor.wordWrap": "on",
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.ignoreRebaseWarning": true,
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"c:\\Users\\aiche\\.vscode\\extensions\\gabrielbb.vscode-lombok-1.0.1\\server\\lombok.jar\"",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "VSBBC",
  "workbench.iconTheme": "vsbbc",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.sideBar.location": "right",
  "explorer.fileNesting.enabled": true,
  "window.zoomLevel": 1
  // "terminal.integrated.defaultProfile.windows": "Command Prompt"
}